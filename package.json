{
  "name": "financier-docdash",
  "version": "0.6.0",
  "description": "A clean, responsive documentation template theme for JSDoc 3 inspired by lodash and minami",
  "main": "publish.js",
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s ../fixtures-doc -f ../fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aeharding/financier-docdash.git"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "jsdoc": "latest",
    "watch-run": "latest"
  },
  "author": "Alexander Harding <aeharding@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template"
  ],
  "dependencies": {
    "@jsdoc/salty": "^0.2.8"
  }
}
